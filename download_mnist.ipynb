    "from keras.datasets import mnist\n",
    "\n",
    "# Get dataset Mnist\n",
    "(trainData,trainLabels),(testData,testLabels)  = mnist.load_data()\n",
    "\n",
    "\n",
    "# save dataset to hard drive\n",
    "import pickle\n",
    "f = open(\"mnistData.pickle\",\"wb\")\n",
    "pickle.dump([trainData,testData,trainLabels,testLabels],f)\n",
    "f.close()\n"
   ]
  }
